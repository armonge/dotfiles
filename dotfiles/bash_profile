#!/bin/bash
#
# ~/.bash_profile
#
# shellcheck disable=SC1090

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi
export PATH="$HOME/.poetry/bin:$PATH"
. "$HOME/.cargo/env"

export GPG_TTY=$(tty)

test -e "${HOME}/.iterm2_shell_integration.bash" && source "${HOME}/.iterm2_shell_integration.bash"

